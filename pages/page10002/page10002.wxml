  <input class = "userinput" name="name" />
</view>
<view class = "userclass">{{university}}
  <textarea auto-height='true' class = "userinput" name="introduction" />
</view>
<view class = "userclass">{{college}}
  <input class = "userinput" name="num" />
</view>
<view class = "userclass">{{department}}
  <input class = "userinput" name="num" />
</view>
<view class = "userclass">学号/教师人事编号
  <input class = "userinput" name="num" />
</view>
<view class = "userclass">{{level}}
  <input class = "userinput" name="num" />
</view>
<view class = "userclass">{{enter_year}}
  <input class = "userinput" name="num" />
</view>

